    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "react-query": "4.0.0-alpha.12",
    "react-redux": "^7.2.6",
    "react-uid": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
