{
  "name": "tasks-app-nextjs",
  "version": "1.0.0",
  "repository": "git@github.com:macchiitaka/tasks-app-nextjs.git",
  "license": "MIT",
  "author": "macchiitaka <zxcvbnmtacashi@gmail.com>",
  "scripts": {
    "build": "pathpida --enableStatic && next build",
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --enableStatic --watch",
    "fmt": "run-s fmt:prettier fmt:package-json fmt:eslint",
    "fmt:prettier": "prettier --write --cache --tab-width 2 \"**/*.{js,jsx,ts,tsx,json,yml,yaml,md}\"",
    "fmt:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache --cache-strategy content --report-unused-disable-directives --fix",
    "fmt:package-json": "sort-package-json",
    "postinstall": "run-p postinstall:**",
    "lint": "run-p lint:**",
    "lint:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache --cache-strategy content",
    "lint:type-check": "tsc -p . --noEmit",
    "postinstall:husky": "is-ci || husky install;",
    "postinstall:pathpida": "pathpida --enableStatic",
    "test": "jest --watch",
    "start": "next start",
    "storybook": "start-storybook -p 6006 -s storybook-public",
    "build-storybook": "build-storybook -s storybook-public"
  },
  "resolutions": {
    "browserslist": "4.21.0",
    "caniuse-lite": "1.0.30001359",
    "immer": "9.0.15"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/material": "^5.8.4",
    "@next/bundle-analyzer": "^12.1.6",
    "@reduxjs/toolkit": "^1.8.2",
    "axios": "0.27.2",
    "next": "12.1.6",
    "next-redux-wrapper": "8.0.0-rc.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "react-query": "4.0.0-beta.23",
    "react-redux": "^8.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@next/eslint-plugin-next": "^12.1.6",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@takamachi/eslint-config": "^2.7.3",
    "@takamachi/prettier-config": "^1.1.0",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@tsconfig/strictest": "^1.0.1",
    "babel-loader": "^8.2.5",
    "browserslist": "4.21.0",
    "caniuse-lite": "1.0.30001359",
    "eslint": "^8.18.0",
    "eslint-config-next": "^12.1.6",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-storybook": "^0.5.13",
    "eslint-plugin-testing-library": "^5.5.1",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "lint-staged": "13.0.3",
    "msw": "^0.42.3",
    "msw-storybook-addon": "^1.6.3",
    "npm-run-all": "4.1.5",
    "pathpida": "0.18.1",
    "pinst": "3.0.0",
    "postcss": "^8.4.14",
    "postcss-jsx": "^0.36.4",
    "postcss-syntax": "^0.36.2",
    "prettier": "2.7.1",
    "react-test-renderer": "^18.2.0",
    "sort-package-json": "1.57.0",
    "storybook-addon-next": "^1.6.6",
    "ts-node": "^10.8.1",
    "typescript": "4.7.4",
    "utility-types": "3.10.0",
    "webpack": "^5.73.0"
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=14.x",
    "yarn": ">=1"
  },
  "msw": {
    "workerDirectory": "storybook-public"
  }
